[flake8]
max-line-length = 100
exclude = 
    .git,
    __pycache__,
    venv,
    .venv,
    build,
    dist,
    *.egg-info,
    alembic/versions

ignore = 
    E203,
    E266,
    E501,
    W503,
    F403,
    F401

per-file-ignores = 
    __init__.py:F401
    
max-complexity = 10

